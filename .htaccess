<IfModule mod_rewrite.c>
    Options -Indexes
    RewriteEngine On
    RewriteBase /inventario/public/

    # Condici√≥n para redirigir si no es un archivo ni un directorio
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^(.*)$ /proyects/inventario/index.php [L]

</IfModule>
